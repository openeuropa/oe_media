{
    "name": "openeuropa/oe_media",
    "description": "Media features for the OpenEuropa project.",
    "type": "drupal-module",
    "license": "EUPL-1.2",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "drupal/core": "^9.2",
        "drupal/entity_browser": "^2.5",
        "drupal/inline_entity_form": "~1.0-rc9",
        "drupal/media_avportal": "^1.0-beta13",
        "drupal/embed": "^1.3",
        "drupal/file_link": "^2.0.4"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "composer/installers": "~1.5",
        "drush/drush": "~10.3",
        "drupal/json_field": "~1.0-rc4",
        "drupaltest/behat-traits": "^0.3",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-dev": "^9.2",
        "guzzlehttp/guzzle": "~6.3",
        "openeuropa/behat-transformation-context": "~0.1",
        "openeuropa/code-review": "^2.0",
        "openeuropa/task-runner-drupal-project-symlink": "^1.0-beta5",
        "phpspec/prophecy-phpunit": "^2",
        "openeuropa/oe_link_lists": "^0.14",
        "openeuropa/oe_webtools": "dev-master",
        "openeuropa/oe_oembed": "~0.1.0",
        "drupal/drupal-extension": "~4.0",
        "egulias/email-validator": "^2.1.22 || ^3.0",
        "nikic/php-parser": "^4.12.0",
        "instaclick/php-webdriver": "^1.4.7",
        "behat/mink-extension": "^2.3.1",
        "drupal/config_devel": "^1.7",
        "twig/twig": "^1.40 || ^2.12",
        "drupal/composite_reference": "^1.0@alpha",
        "drupal/entity_reference_revisions": "^1.7"
    },
    "scripts": {
        "post-install-cmd": "./vendor/bin/run drupal:site-setup",
        "post-update-cmd": "./vendor/bin/run drupal:site-setup"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "autoload": {
        "psr-4": {
            "Drupal\\oe_media\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\oe_media\\": "./tests/src",
            "Drupal\\Tests\\oe_media_embed\\": "./modules/oe_media_embed/tests/src",
            "Drupal\\Tests\\oe_link_lists\\": "./build/modules/contrib/oe_link_lists/tests/src",
            "Drupal\\Tests\\oe_link_lists_manual_source\\": "./build/modules/contrib/oe_link_lists/modules/oe_link_lists_manual_source/tests/src"
        }
    },
    "_readme": [
        "Explicit requirement of behat/behat module due to PHP8 compatibility.",
        "Explicit requirement of behat/mink-extension to assure lower compatibility.",
        "Explicit requirement for egulias/email-validator due to https://www.drupal.org/project/drupal/issues/3061074#comment-14300579. It can be removed when Drupal core 9.2 support is droppped."
    ],
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "installer-paths": {
            "build/core": [
                "type:drupal-core"
            ],
            "build/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "build/modules/contrib/{$name}": [
                "type:drupal-module"
            ]
        },
        "patches": {},
        "drupal-scaffold": {
            "locations": {
                "web-root": "./build"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "phpro/grumphp": true
        }
    }
}
